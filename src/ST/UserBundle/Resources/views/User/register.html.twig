{% extends 'STUserBundle::base.html.twig' %}
{% block body %}
<div class="panel-heading">
<h3>Register!</h3>
</div>
<div class="panel-body">
{{ form_start(form) }}
	{{ form_widget(form.username, {'attr': {'style': 'margin: 10px 0px 10px 0px'} }) }}
	{{ form_widget(form.file) }}
	{{ form_widget(form.plainPassword.first, {'attr': {'style': 'margin: 10px 0px 10px 0px','placeholder ':'Password'} }) }}
	{{ form_widget(form.plainPassword.second, {'attr': {'style': 'margin: 10px 0px 10px 0px','placeholder ':'Password'} }) }}
	<button type="submit" class="btn btn-default" formnovalidate>Register</button>
{{ form_end(form) }}
</div>
{% endblock %}
